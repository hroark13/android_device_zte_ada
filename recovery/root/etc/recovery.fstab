#device 	fstype		mount point		[device2] [length=]

/dev/block/platform/msm_sdcc.1/by-name/system       /system         ext4    rw,barrier=1                                                    wait
/dev/block/platform/msm_sdcc.1/by-name/cache        /cache          ext4    noatime,nosuid,nodev,barrier=1,data=ordered                     wait,check
/dev/block/platform/msm_sdcc.1/by-name/userdata     /data           ext4    noatime,nosuid,nodev,barrier=1,data=ordered,noauto_da_alloc     wait,check,encryptable=footer,length=-16384
#/dev/block/mmcblk1                                  /sdcard     vfat    defaults       defaults

# External SD
/dev/block/mmcblk1 /external_sd auto defaults recoveryonly,lun=/sys/devices/platform/msm_hsusb/gadget/lun0/file
